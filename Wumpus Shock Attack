var emptyArray = [];
var currentDelay = 0;
var curBlock;
var curPermutation = BlockPermutation.resolve("minecraft:gold_block");
var originalBlockTypes = [[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],
[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],[],];
var count = 0;
var playerDelay = 0;

for (let i = 1; i <= 50; i++) {
    for (let j = 1; j <= 50; j ++) {
        curBlock = world.getDimension("overworld").getBlock({x: i, y: -60, z: j});

        curPermutation = curBlock.permutation;
       
        count++;

      //  world.sendMessage(count.toString());

        originalBlockTypes[i].push(curPermutation);
    }
}
world.sendMessage(".");
var curPermutation = BlockPermutation.resolve("minecraft:gold_block");

for (let i = 0 ; i <= 50; i++) {
  //  emptyArray.push(currentDelay);
}
world.sendMessage(emptyArray.length.toString());
for (let i = 0; i <= 50; i++) {
 //   blockDelay.push(emptyArray);
}
var curY = -60;

var seaLanternPermutation = BlockPermutation.resolve("minecraft:sea_lantern");
function ShockInitialize () {
    for (var x = 1; x <= 50; x += 3) {
        for (var z = 1; z <= 50; z += 3) {
            var randNumber = Math.ceil(Math.random() * 5);

            if (randNumber == 1) {
            //    world.sendMessage("in");
                for (var startX = x; startX < x + 3; startX++) {
                    for (var startZ = z; startZ < z + 3; startZ ++) {
                        curBlock = world.getDimension("overworld").getBlock({x: startX, y: curY, z: startZ});

                        curBlock.setPermutation(curPermutation);

                        
                    }
                }
            }
        }
    }
}
function ShockDeliver () {
  //  world.sendMessage(blockDelay[0][0].toString());
    for (var startx = 1; startx <= 50; startx++) {
        for (var startz = 1; startz <= 50; startz++) {
            curBlock = curBlock = world.getDimension("overworld").getBlock({x: startx, y: curY, z: startz});
            
            if (curBlock.permutation == "undefined") {
                continue;
            }
            if (curBlock.permutation == curPermutation || curBlock.permutation == seaLanternPermutation) {
                blockDelay[startx][startz]++;

             //   world.sendMessage(blockDelay[startx][startz].toString());
                
                if (blockDelay[startx][startz] == 75) {
                    curBlock.setPermutation(BlockPermutation.resolve("minecraft:sea_lantern"));
                }
                if (blockDelay[startx][startz] == 100) {
                 //   world.sendMessage("in");

                    curBlock.setPermutation(BlockPermutation.resolve("minecraft:stone"));

                    blockDelay[startx][startz] = 0;
                }
            }
        }
    }
}
function DamagePlayer () {
    curBlock = world.getDimension("overworld").getBlock({x: Math.floor(player.location.x), y: -60, z: Math.floor(player.location.z)});
    if (curBlock.permutation == seaLanternPermutation) {
        world.getDimension("overworld").runCommand("damage @p 6");
    }
}

function mainTick () {
    if (system.currentTick % 1 == 0) {
        if (system.currentTick % 200 == 0) {
       //     world.sendMessage("1");
            system.run(ShockInitialize);
        }
        system.run(ShockDeliver);
        if (system.currentTick % 10 == 0) {
            system.run(DamagePlayer);
        }
        
    } 
    system.run(mainTick);
}

system.run(mainTick);
